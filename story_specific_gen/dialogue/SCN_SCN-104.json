      "nodes": [
        {
          "id": "N_START",
          "next": "N_HUB",
          "speakerId": "NARRATOR",
          "text": "Staging stations sit in marked zones: reader-loop; dummy-mass fixture; test-load panel; clipboard-station. Photographed mid-compliance.",
          "type": "npc_line"
        },
        {
          "choices": [
            {
              "id": "C_LOOK_STATIONS",
              "next": "N_NARR_LOOK_STATIONS",
              "text": "look: Scan the staging stations."
            },
            {
              "id": "C_USE_AMR_MENU",
              "next": "N_AMR_MENU",
              "text": "use: Go to the AMR reader loop."
            },
            {
              "id": "C_USE_DUMMY_MENU",
              "next": "N_DUMMY_MENU",
              "text": "use: Go to the dummy mass fixture."
            },
            {
              "id": "C_USE_LOAD_MENU",
              "next": "N_LOAD_MENU",
              "text": "use: Go to the test load panel."
            },
            {
              "id": "C_USE_CLIP_MENU",
              "next": "N_CLIP_MENU",
              "text": "use: Go to the supervisor clipboard station."
            },
            {
              "conditions": [
                {
                  "key": "act2.staging.amrClipped",
